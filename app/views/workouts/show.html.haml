#workout
	%p= @workout.date


#songs
	
	%h2#songheader Songs
	= render @workout.songs

	%h3#songheader Add a Song
	= render "songs/form"




= link_to "Back", root_path, id: "cancelbutton"
= link_to "Edit", edit_workout_path(@workout), id: "cancelbutton"
= link_to "Delete", workout_path(@workout), method: :delete, data: {confirm: "Are you sure?"}, id: "cancelbutton",